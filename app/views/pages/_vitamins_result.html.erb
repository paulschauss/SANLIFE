  <div data-illness-preferences-results-target="results" data-action="refreshResult@window->illness-preferences-results#refresh">
    <% @nutrients.each do |nutrient| %>
      <p><%= nutrient.name %></p>
    <% end %>
    <h2>Weekly Plan</h2>
    <div class="line-height">
    </div>
    <% @nutrients.each do |nutrient| %>
      <div class="nutrient-food-row">
        <h2 class="nutrient"><strong><%= nutrient.name %></strong></h2>
        <% nutrient.foods.sample(4).each do |food| %>
          <%= render "foods/food_cards", food: food %>
        <% end %>
      </div>
    <% end %>
  </div>
